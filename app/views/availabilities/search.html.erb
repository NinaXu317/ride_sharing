<p id="notice" class = "container text-center" style="padding-top:5px"><%= notice %></p>

<div id = "search_page">
    <div id = "search_form", style="margin-top: 2%; margin-left: 4%; margin-right: 4%;">
        <%= render 'searchform' %>
    </div>
    <div id = "result", style="margin-top: 4%; margin-left: 4%; margin-right: 4%;">
        <div id="results">
            <% if @availabilities %>
                <%= render 'result', :availabilities => @availabilities %>
            <% else %>
                <%= render 'noresult' %>
             <% end %>
        </div>
    </div>
</div>
