<% content_for :content do %>

  <%= form_tag sessions_path do %>
    <%= label_tag "email" %>
    <%= text_field_tag :email, nil, placeholder: "Email" %>
    <%= label_tag "Password" %>
    <%= password_field_tag :password, nil, placeholder: "Password" %>
    <%= submit_tag "Log In"%>
  <% end %>

<% end %>